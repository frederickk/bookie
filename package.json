{
  "name": "bookie",
  "version": "0.1.8",
  "description": "Bookie is a Chrome Extension that makes a curated set of bookmarks and additional notes accessible within the Chrome menubar.",
  "main": "menu.html",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"NODE_ENV=development node_modules/.bin/webpack --watch\" \"webpack-dev-server --host 0.0.0.0 --port 1112\"",
    "build": "NODE_ENV=production node_modules/.bin/webpack && npm run package",
    "build:chrome": "NODE_ENV=production node_modules/.bin/webpack && npm run package:chrome",
    "build:firefox": "NODE_ENV=production node_modules/.bin/webpack && npm run package:firefox",
    "package": "./package.sh bookie all",
    "package:chrome": "./package.sh bookie chrome",
    "package:firefox": "./package.sh bookie firefox",
    "publish": "NODE_ENV=production npm run package && node ./chrome-upload.js",
    "push": "git push origin master"
  },
  "author": "Ken Frederick <frederickk@google.com>",
