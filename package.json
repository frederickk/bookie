{
  "name": "bookie",
  "version": "0.1.13",
  "description": "Bookie is a Chrome Extension that makes a curated set of bookmarks and additional notes accessible within the Chrome menubar.",
  "main": "popup.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/frederickk/bookie"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"NODE_ENV=production webpack --watch\" \"webpack serve\"",
    "build": "NODE_ENV=production webpack",
    "package": "npm run clear && npm run build && ./package.sh bookie all",
    "package:chrome": "npm run clear && npm run build && ./package.sh bookie chrome",
    "package:firefox": "npm run clear && npm run build && ./package.sh bookie firefox",
    "clear": "rm bookie-chrome.zip bookie-firefox.zip",
    "publish": "NODE_ENV=production npm run package && node ./chrome-upload.js",
    "push": "git push origin master"
  },
  "author": "Ken Frederick <ken.frederickk@gmx.de>",
  "license": "UNLICENSED",
